@{
    ViewData["Title"] = "Stylists";
}

<h2>All of our stylists!</h2>

<a href="/stylists/add"><button>Add a stylist</button></a>


<div class="stylistsGrid">


    @foreach (var stylist in Model)
    {
        <div>
            <a href="/stylists/@stylist.Id">@stylist.Name</a>
            <form target="/stylists/@stylist.Id/delete" method="post"><button>Delete</button></form>
        </div>
    }
</div>







<a href="/"><button>Home</button></a>